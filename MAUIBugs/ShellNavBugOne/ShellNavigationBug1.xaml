             x:Class="MAUIBugs.ShellNavBugOne.ShellNavigationBug1"
       xmlns:pages="clr-namespace:MAUIBugs.ShellNavBugOne">

    <TabBar Route="sampleApp">
        <Tab Title="First Page" Route="first">
            <ShellContent ContentTemplate="{DataTemplate pages:FirstPage}" />
        </Tab>
        <Tab Title="Settings" Route="settings">
            <ShellContent ContentTemplate="{DataTemplate pages:SettingsPage}" />
        </Tab>
    </TabBar>
    
</Shell>
